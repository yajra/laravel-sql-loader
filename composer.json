{
    "name": "yajra/laravel-sql-loader",
    "description": "Oracle SQL Loader for Laravel",
    "keywords": [
        "laravel",
        "oracle",
        "oci8",
        "sqlldr"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Arjay Angeles",
            "email": "aqangeles@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-oci8": ">=3.0.1",
        "ext-pdo": "*",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0",
        "yajra/laravel-oci8": "^10.6|^11.3"
    },
    "require-dev": {
        "larastan/larastan": "^2.9.7",
        "laravel/pint": "^1.16",
        "mockery/mockery": "^1.6.12",
        "orchestra/testbench": "^9.1.1",
        "pestphp/pest": "^4.1.0",
        "pestphp/pest-plugin-laravel": "*",
        "rector/rector": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Yajra\\SQLLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yajra\\SQLLoader\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Yajra\\SQLLoader\\SQLLoaderServiceProvider"
            ]
        }
    },
    "scripts": {
        "docker": "docker run -d -p 49160:22 -p 49161:1521 deepdiver/docker-oracle-xe-11g",
        "pint": [
            "./vendor/bin/pint"
        ],
        "stan": [
            "./vendor/bin/phpstan analyse --memory-limit=2G --ansi --no-progress --no-interaction --configuration=phpstan.neon.dist"
        ],
        "rector": [
            "./vendor/bin/rector"
        ],
        "test": [
            "./vendor/bin/pest"
        ],
        "qa": [
            "composer pint",
            "composer rector",
            "composer stan",
            "composer test"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
